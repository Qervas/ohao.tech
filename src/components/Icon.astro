---
// Import the React wrapper component
import IconWrapper from './IconWrapper';

interface Props {
  name: string;
  className?: string;
  [key: string]: any;
}

const { name, className = '', ...props } = Astro.props;
---

<IconWrapper
  client:only="react"
  name={name}
  className={`inline-block ${className}`}
  {...props}
/>
